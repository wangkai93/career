<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="cn.edu.zzia.career.pojo.CmUnemp" table="cm_unemp" schema="career" dynamic-insert="true" dynamic-update="true">
        <id name="ueid" type="java.lang.Integer">
            <column name="ueid" />
            <generator class="native" />
        </id>
        <property name="uesalary" column="uesalary" not-null="false"/>
        <property name="uetime" column="uetime" not-null="false"/>
        <property name="ueschool" column="ueschool" not-null="false"/>
        <property name="uemajor" column="uemajor" not-null="false"/>
        <property name="uesuccess" column="uesuccess" not-null="false"/>
        <property name="ujobName" column="ujob_name" not-null="false"/>
        <property name="uestate" column="uestate" not-null="false"/>
        <many-to-one name="cmStudentBySid" class="cn.edu.zzia.career.pojo.CmStudent">
            <column name="sid"/>
        </many-to-one>
        <many-to-one name="cmDirectionByDid" class="cn.edu.zzia.career.pojo.CmDirection">
            <column name="did"/>
        </many-to-one>
        <many-to-one name="cmJobByJid" class="cn.edu.zzia.career.pojo.CmJob" lazy="false">
            <column name="jid"  not-null="false"/>
        </many-to-one>
    </class>
</hibernate-mapping>